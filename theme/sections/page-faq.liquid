{%- style -%}
  .product-detail-title {
    font-family: Poppins;
    font-size: 16px;
    font-weight: 600;
    line-height: 24px;
    letter-spacing: 0;
    text-align: left;
  }
  .faq-section {
    padding: 15px 45px !important;
  }
  .faq-title {
    font-family: Tiempos Headline;
    font-size: 54px;
    font-weight: 700;
    line-height: 65px;
    letter-spacing: 0;
    text-align: left;
    color: #191919;
  }
  @media screen and (max-width: 750px) {
    .faq-section {
      padding: 5px 15px !important;
    }
  }
{%- endstyle -%}
